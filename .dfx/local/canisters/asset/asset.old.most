// Version: 1.0.0
type Asset =
  {
    assetType : AssetType;
    chunkIds : [ChunkId];
    contentType : Text;
    createdAt : Int;
    filename : Text;
    id : AssetId;
    owner : Principal
  };
type AssetChunk = {data : Blob; id : ChunkId};
type AssetId = Text;
type AssetType = {#FilterImage; #MainImage; #Other};
type ChunkId = Nat;
actor {
  stable var assetsArray : [(AssetId, Asset)];
  stable var chunksArray : [(ChunkId, AssetChunk)];
  stable var nextChunkId : Nat
};
